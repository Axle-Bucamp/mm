{
  "mcpServers": {
    "desktop-commander": {
      "command": "npx",
      "args": [
        "-y",
        "@wonderwhy-er/desktop-commander"
      ]
    },
    "playwright": {
      "command": "npx",
      "args": [
        "-y",
        "@executeautomation/playwright-mcp-server",
        "--browser",
        "chromium"
      ]
    },
    "zeldaret-mm": {
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-v", "${PWD}:/project",
        "-w", "/project",
        "ghcr.io/zeldaret/mm",
        "bash"
      ]
    }
  }
}
